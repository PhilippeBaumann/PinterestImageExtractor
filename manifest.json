{
  "name": "Pinterest Image Extractor",
  "action": {},
  "manifest_version": 3,
  "version": "0.2",
  "description": "Click the Icon to download all images from the board",
  "icons": {
      "32": "icon32.png",
      "48": "icon48.png",
      "128": "icon128.png" },
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "background": {
    "service_worker": "main.js"
  }
}